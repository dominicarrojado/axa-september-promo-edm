  <mj-body width="800px" background-color="#d7e1e9">
    <mj-wrapper css-class="wrapper" padding="40px 0">
      <!-- START of Hero Image 1 -->
      <mj-section>
        <mj-group>
          <mj-column width="100%">
            <mj-image
              src="images/banner-axa-smart-travel.png"
              alt="AXA Adventures abound with every trip - 3-Days Only! (From 25 to 27 Sep 2017) 50% off* Single Trip Plan with AXA SmartTraveller Insurance - couple seated on a cliff"
            />
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Hero Image 1 -->

      <!-- START of Content 1 -->
      <mj-section background-color="#133483" padding="60px 20px">
        <mj-group>
          <mj-column width="100%">
            <mj-text align="center" color="#ffffff" font-size="22px">
              Have an upcoming trip? Travel the World with complete
            </mj-text>
            <mj-text align="center" color="#ffffff" font-size="22px">
              peace of mind with AXA SmartTraveller Insurance.
            </mj-text>
            <mj-text align="center" color="#ffffff" font-size="22px">
              From 25 to 27 Sept 2017, get 50% off* Single Trip Plan.
            </mj-text>
            <mj-button
              href="https://www.axa.com.sg/travel-insurance/smarttraveller"
              rel="noopener noreferrer nofollow"
              css-class="btn-hover-white"
              align="center"
              padding-top="30px"
              border="2px solid #ffffff"
              border-radius="0"
              background-color="transparent"
              inner-padding="9px 50px"
              color="#ffffff"
              font-size="16px"
              font-weight="bold"
              text-transform="uppercase"
            >
              Buy Now
            </mj-button>
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Content 1 -->

      <!-- START of Hero Image 2 -->
      <mj-section>
        <mj-group>
          <mj-column width="100%">
            <mj-image
              src="images/banner-axa-smart-home.png"
              alt="AXA Safeguard what means most to you - 40% off* AXA SmartHome Insurance, mother and kid having fun building legos"
            />
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Hero Image 2 -->

      <!-- START of Content 3 -->
      <mj-section background-color="#f5f5f5" padding="60px 20px">
        <mj-group>
          <mj-column width="100%">
            <mj-text align="center" color="#2e2e2e" font-size="22px">
              Enjoy 40% off* AXA SmartHome Insurance and
            </mj-text>
            <mj-text align="center" color="#2e2e2e" font-size="22px">
              get covered for your home contents and more.
            </mj-text>
            <mj-button
              href="https://www.axa.com.sg/home-insurance"
              rel="noopener noreferrer nofollow"
              css-class="btn-hover-blue"
              align="center"
              padding-top="30px"
              border="2px solid #133483"
              border-radius="0"
              background-color="transparent"
              inner-padding="9px 50px"
              color="#133483"
              font-size="16px"
              font-weight="bold"
              text-transform="uppercase"
            >
              Buy Now
            </mj-button>
          </mj-column>
        </mj-group>
      </mj-section>
      <!-- END of Content 3 -->

      <!-- START of Footer -->
      <mj-section background-color="#133483" padding="0 20px 18px">
        <mj-column width="16%" vertical-align="middle" padding-top="20px">
          <mj-button
            href="https://www.axa.com.sg/"
            rel="noopener noreferrer nofollow"
            css-class="btn-text-hover"
            align="center"
            background-color="transparent"
            inner-padding="0"
            color="#ffffff"
            font-size="12px"
            font-weight="bold"
            text-transform="uppercase"
